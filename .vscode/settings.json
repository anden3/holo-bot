{
    "lldb.showDisassembly": "auto",
    "lldb.dereferencePointers": true,
    "lldb.displayFormat": "auto",
    "files.watcherExclude": {
        "**/target/**": true
    },
    "TARGET_ARCH": "aarch64-unknown-linux-musl",
    "TARGET_HOST": "pi@rpi4b",
    "TARGET_EXEC": "holo-bot",
    "TARGET_PATH": "/home/pi/Documents/Rust/${config:TARGET_EXEC}",
    "cSpell.ignoreWords": [
        "jfvk",
        "tftbi",
        "πέκο",
        "пеко",
        "بيكو",
        "პეკო"
    ],
    "rust-analyzer.diagnostics.disabled": [
        "unresolved-macro-call"
    ],
    "evenBetterToml.schema.associations": {
        ".*holobot\\.toml": "./schemas/holobot-config-schema.json",
        ".*talents\\.toml": "./schemas/holobot-talents-schema.json"
        // ".*holobot\\.toml": "https://gist.githubusercontent.com/anden3/ef3c3be4a5ad6d35fff1bfb40252e621/raw/a7302728e7dc41ae520251eb6aa0faf438a3b634/holobot-config-schema.json"
    }
}